{
  "manifest_version": 3,
  "name": "SystemTapped",
  "version": "0.1",
  "description": "Plugin f√∂r Systembolaget och Untappd.",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["popup.html", "popup.js"],
      "matches": ["*://*/*"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon.png"
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  }
}
